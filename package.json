    "@faceless-ui/scroll-info": "^1.2.5",
    "@faceless-ui/window-info": "^1.2.7",
    "color": "^3.1.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jss-global": "^3.0.0",
    "next": "^10.0.5",
    "payload": "^0.9.4",
    "react": "^17.0.1",
    "react-jss": "10.4.0",
    "react-toastify": "^7.0.4",
    "react-transition-group": "^4.4.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@trbl/eslint-config": "^2.0.3",
    "@types/express": "^4.17.11",
    "@types/html-validator": "^5.0.0",
